<% form_for @author do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :publication_id %><br />
    <%= f.text_field :publication_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :country_team_id %><br />
    <%= f.text_field :country_team_id %>
  </p>
  <p>
    <%= f.label :focus_group_id %><br />
    <%= f.text_field :focus_group_id %>
  </p>
  <p>
    <%= f.label :first_author %><br />
    <%= f.check_box :first_author %>
  </p>
  <p><%= f.submit %></p>
<% end %>
